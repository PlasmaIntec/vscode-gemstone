{
	"name": "gemstone",
	"displayName": "GemStone",
	"description": "GemStone/S 64 Bit tools for Visual Studio Code",
	"version": "0.0.1",
	"publisher": "jgfoster.net",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "gemstone.addLogin",
				"title": "Add"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "gemstone.addLogin",
					"when": "view == gemstone-login"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "gemstone",
					"title": "GemStone/S Tools",
					"icon": "media/sparkle32.png"
				}
			]
		},
		"views": {
			"gemstone": [
				{
					"id": "gemstone-login",
					"name": "Logins"
				},
				{
					"id": "gemstone-sessions",
					"name": "Sessions"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"tslint": "^5.17.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"gci-js": "^1.0.2"
	}
}
